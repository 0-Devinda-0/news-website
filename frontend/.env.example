# Auth0 Configuration
AUTH0_SECRET=your_auth0_secret_here
AUTH0_DOMAIN=https://your-tenant-region.auth0.com
AUTH0_CLIENT_ID=your_auth0_client_id_here
AUTH0_CLIENT_SECRET=your_auth0_client_secret_here

# Application URL
APP_BASE_URL=http://localhost:3000

# Auth0 Issuer (optional, used for Management API or token verification)
AUTH0_ISSUER_BASE_URL=https://your-tenant-region.auth0.com
AUTH0_ISSUER_CLIENT_ID=your_issuer_client_id_here
AUTH0_ISSUER_CLIENT_SECRET=your_issuer_client_secret_here